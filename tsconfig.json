{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "outDir": "dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  },
  "include": [
    "./*.ts"
  ],
  "exclude": [
	"dist/"
  ]
}
